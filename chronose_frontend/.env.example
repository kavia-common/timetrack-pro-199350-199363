# Chronose Frontend - Example environment variables
# Copy to .env and fill in appropriate values.

# App URLs
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_BACKEND_URL=
REACT_APP_API_BASE=
REACT_APP_WS_URL=

# Build/runtime behavior
REACT_APP_NODE_ENV=development
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/healthz

# Feature flags (JSON or CSV key=value)
# Default enableRealData=true when Supabase is configured.
# Example JSON: {"enableRealData": true}
# Example CSV: enableRealData=true
REACT_APP_FEATURE_FLAGS={"enableRealData": true}
REACT_APP_EXPERIMENTS_ENABLED=false

# Supabase configuration (required for real data/auth)
REACT_APP_SUPABASE_URL=
REACT_APP_SUPABASE_KEY=
