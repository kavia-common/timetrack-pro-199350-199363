# Chronose Frontend - Environment Variables Example
# Copy this file to `.env` and fill in values for your environment.

# Application
REACT_APP_NODE_ENV=development
REACT_APP_PORT=3000
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/healthz
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_TRUST_PROXY=false

# URLs
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_API_BASE=
REACT_APP_BACKEND_URL=
REACT_APP_WS_URL=

# Feature flags
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=false
REACT_APP_ENABLE_MICROSOFT_SSO=false

# Supabase
# REQUIRED: Your Supabase project URL and anon/public key.
REACT_APP_SUPABASE_URL=
REACT_APP_SUPABASE_KEY=

# Optional: Used for auth email redirect on sign-up; falls back to FRONTEND_URL or window.location.origin
REACT_APP_SITE_URL=

# Note: REACT_APP_SUPABASE_ANON_KEY has been replaced by REACT_APP_SUPABASE_KEY.
# If you previously used REACT_APP_SUPABASE_ANON_KEY, rename it to REACT_APP_SUPABASE_KEY.
