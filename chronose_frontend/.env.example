# Frontend environment configuration (do NOT commit .env with secrets)
REACT_APP_NODE_ENV=development
REACT_APP_PORT=3000

# Base URLs (if applicable for other services)
REACT_APP_API_BASE=
REACT_APP_BACKEND_URL=
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_WS_URL=

# Feature flags
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=false

# Supabase configuration (Required)
REACT_APP_SUPABASE_URL=
REACT_APP_SUPABASE_ANON_KEY=

# Email redirect after sign-up (optional; defaults to window.origin)
REACT_APP_SITE_URL=http://localhost:3000

# SSO feature toggles
REACT_APP_ENABLE_MICROSOFT_SSO=false
