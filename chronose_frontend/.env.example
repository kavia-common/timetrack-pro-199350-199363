# Environment variables for Chronose Frontend (React)
# Do NOT commit real secrets here. Copy to .env and fill with real values.

# App URLs
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_BACKEND_URL=
REACT_APP_API_BASE=
REACT_APP_WS_URL=

# App configuration
REACT_APP_NODE_ENV=development
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=false

# Supabase configuration (required for real auth)
REACT_APP_SUPABASE_URL=
REACT_APP_SUPABASE_KEY=

# Optional flags
REACT_APP_ENABLE_MICROSOFT_SSO=false
