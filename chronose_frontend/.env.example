# .env.example
# Example environment variables for Chronose Frontend (DO NOT use for prod secrets)
REACT_APP_API_BASE=https://api.example.com
REACT_APP_BACKEND_URL=https://backend.example.com
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_WS_URL=ws://localhost:3000/ws

REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=0
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_FEATURE_FLAGS=ALLOW_LEAVE,OVERTIME
REACT_APP_EXPERIMENTS_ENABLED=false
REACT_APP_SUPABASE_URL=https://YOUR_SUPABASE_URL.supabase.co
REACT_APP_SUPABASE_KEY=YOUR_SUPABASE_ANON_KEY
