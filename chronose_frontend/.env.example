# Example environment variables for Chronose Frontend (React)
# Copy this file to ".env" and fill the values as needed.

# PUBLIC_INTERFACE
# REACT_APP_SITE_URL: The full site URL used for auth email redirects
REACT_APP_SITE_URL=http://localhost:3000

# Supabase credentials (required for authentication)
# PUBLIC_INTERFACE
REACT_APP_SUPABASE_URL=
# PUBLIC_INTERFACE
REACT_APP_SUPABASE_ANON_KEY=

# Optional flags and URLs used by the app or deployment
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_BACKEND_URL=
REACT_APP_API_BASE=
REACT_APP_WS_URL=
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=false
REACT_APP_ENABLE_MICROSOFT_SSO=false
